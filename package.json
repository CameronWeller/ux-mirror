{
  "name": "ux-mirror",
  "version": "1.0.0",
  "description": "Self-Programming GPU-Driven UX Intelligence System - Where user experience data becomes the mirror for AI self-improvement",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "start:core": "python start_core_system.py",
    "start:simple-orchestrator": "python agents/simple_orchestrator.py",
    "start:orchestrator": "python agents/core_orchestrator.py",
    "start:visual-analysis": "python agents/visual_analysis_agent.py",
    "start:metrics-intelligence": "python agents/metrics_intelligence.py",
    "start:autonomous-implementation": "python agents/autonomous_implementation.py",
    "start:metrics-collection": "node src/metrics-collection/index.js",
    "start:all-agents": "concurrently \"ux-tester agent start orchestrator\" \"ux-tester agent start visual\" \"ux-tester agent start metrics\"",
    "start:legacy-agents": "concurrently \"npm run start:orchestrator\" \"npm run start:metrics-intelligence\" \"npm run start:visual-analysis\" \"npm run start:autonomous-implementation\"",
    "start:monitoring": "ux-tester monitor start",
    "stop:monitoring": "ux-tester monitor stop",
    "agent:start": "ux-tester agent start all",
    "agent:stop": "ux-tester agent stop all",
    "agent:status": "ux-tester agent status",
    "insights": "ux-tester insights",
    "monitor:status": "ux-tester monitor status",
    "dev": "nodemon src/index.js",
    "build": "webpack --mode=production",
    "build:web": "webpack --config webpack.web.config.js",
    "build:electron": "electron-builder",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/ --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint src/ --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write src/",
    "type-check": "tsc --noEmit",
    "setup:gpu-pipeline": "python setup_gpu_pipeline.py",
    "docker:build": "docker build -t ux-mirror .",
    "docker:run": "docker run -p 8765:8765 -p 3000:3000 ux-mirror",
    "deploy:web": "npm run build:web && node deploy/web-deploy.js",
    "deploy:desktop": "npm run build:electron && node deploy/desktop-deploy.js",
    "deploy:mobile": "react-native run-android && react-native run-ios",
    "metrics:dashboard": "node src/dashboard/server.js",
    "analytics:export": "node src/analytics/export-data.js",
    "clean": "rimraf dist/ build/ node_modules/.cache/",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "ux",
    "ai",
    "machine-learning",
    "gpu",
    "self-programming",
    "user-experience",
    "analytics",
    "computer-vision",
    "real-time",
    "cross-platform"
  ],
  "author": "UX-MIRROR System",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ux-mirror.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/ux-mirror/issues"
  },
  "homepage": "https://github.com/your-org/ux-mirror#readme",
  "dependencies": {
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow/tfjs-node-gpu": "^4.10.0",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "ws": "^8.14.2",
    "sharp": "^0.32.6",
    "puppeteer": "^21.3.8",
    "playwright": "^1.39.0",
    "selenium-webdriver": "^4.15.0",
    "opencv4nodejs": "^5.6.0",
    "jimp": "^0.22.10",
    "canvas": "^2.11.2",
    "node-screenshots": "^0.0.16",
    "robotjs": "^0.6.0",
    "nut-js/nut-js": "^4.2.0",
    "systeminformation": "^5.21.15",
    "node-machine-id": "^1.1.12",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "date-fns": "^2.30.0",
    "rxjs": "^7.8.1",
    "eventemitter3": "^5.0.1",
    "async": "^3.2.4",
    "p-queue": "^7.4.1",
    "redis": "^4.6.10",
    "sqlite3": "^5.1.6",
    "mysql2": "^3.6.3",
    "mongodb": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "config": "^3.3.9",
    "yargs": "^17.7.2",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.11",
    "commander": "^11.1.0",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/express": "^4.17.20",
    "@types/ws": "^8.5.8",
    "@types/uuid": "^9.0.6",
    "@types/lodash": "^4.14.200",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/bcryptjs": "^2.4.5",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.4",
    "@types/morgan": "^1.9.7",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.0",
    "vue": "^3.3.0",
    "@vue/cli": "^5.0.0",
    "angular": "^16.0.0",
    "@angular/cli": "^16.0.0"
  },
  "optionalDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "react-native": "^0.72.0",
    "@react-native-community/cli": "^11.3.0",
    "expo": "^49.0.0",
    "@expo/cli": "^0.10.0",
    "unity-webgl": "^1.0.0",
    "unreal-js": "^1.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"],
    "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js"],
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  },
  "electron": {
    "main": "dist/electron/main.js",
    "homepage": "./",
    "directories": {
      "output": "dist/electron/"
    }
  },
  "build": {
    "appId": "com.uxmirror.app",
    "productName": "UX-MIRROR",
    "directories": {
      "output": "dist/electron/"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "arm64"]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "deb",
          "arch": ["x64", "arm64"]
        }
      ]
    }
  },
  "ux-mirror": {
    "version": "1.0.0",
    "agents": {
      "core_orchestrator": {
        "port": 8765,
        "gpu_allocation": 0.1
      },
      "metrics_intelligence": {
        "gpu_allocation": 0.3,
        "collection_interval": 100
      },
      "visual_analysis": {
        "gpu_allocation": 0.4,
        "analysis_interval": 500
      },
      "autonomous_implementation": {
        "gpu_allocation": 0.2,
        "deployment_queue_size": 10
      }
    },
    "platforms": {
      "web": {
        "enabled": true,
        "port": 3000
      },
      "desktop": {
        "enabled": true,
        "auto_launch": false
      },
      "mobile": {
        "enabled": true,
        "react_native": true
      },
      "games": {
        "enabled": true,
        "unity_plugin": true,
        "unreal_plugin": true
      }
    },
    "gpu": {
      "preferred_vendor": "nvidia",
      "fallback_to_cpu": true,
      "memory_limit": "8GB",
      "compute_capability": "7.0"
    },
    "metrics": {
      "real_time_enabled": true,
      "buffer_size": 10000,
      "export_format": "json",
      "dashboard_enabled": true
    }
  }
} 